transition: transform 0.2s linear;\r\n  bottom: -6px;\r\n}\r\n\r\n.burgermenu_toggle::before {\r\n  content: \"\";\r\n  width: 100%;\r\n  height: 3px;\r\n  background-color: #fff;\r\n  position: absolute;\r\n  left: 0;\r\n  z-index: 1;\r\n  transition: transform 0.2s linear;\r\n  bottom: 6px;\r\n}\r\n\r\n.navitems.active {\r\n  display: block;\r\n\r\n  position: absolute;\r\n  background: white;\r\n  border: 1px solid;\r\n  padding: 0 10px 0 20px;\r\n  border-top: none;\r\n  top: 50px;\r\n  z-index: 10;\r\n}\r\n\r\n.burgermenu.active .burgermenu_toggle {\r\n  background: none;\r\n}\r\n\r\n.burgermenu.active .burgermenu_toggle::before {\r\n  transform: rotate(45deg) translateX(-2px);\r\n  transform-origin: left top;\r\n}\r\n\r\n.burgermenu.active .burgermenu_toggle::after {\r\n  transform: rotate(-45deg) translateX(-3px);\r\n  transform-origin: left bottom;\r\n}\r\n\r\n/* .slider */\r\n.slider-container {\r\n  overflow: hidden;\r\n  border-radius: 20px;\r\n}\r\n\r\n.slider-track {\r\n  display: flex;\r\n  background-color: yellow;\r\n  transition: 0.2s;\r\n}\r\n\r\n.slider-item {\r\n  object-fit: contain;\r\n  height: 370px;\r\n}\r\n\r\n.slider-item div {\r\n  background: #000;\r\n  overflow: hidden;\r\n}\r\n\r\n.slider-item img {\r\n  opacity: 1;\r\n  transition: opacity 124ms linear, transform 124ms linear;\r\n  display: block;\r\n}\r\n\r\n.slider-item:hover div img {\r\n  cursor: pointer;\r\n  /* opacity: 0.5;\r\n  transform: scale(1.05); */\r\n}\r\n\r\n/* .slider-item:nth-child(3n + 1) {\r\n  background-color: blue;\r\n}\r\n\r\n.slider-item:nth-child(3n + 2) {\r\n  background-color: red;\r\n}\r\n\r\n.slider-item:nth-child(3n + 3) {\r\n  background-color: green;\r\n} */\r\n\r\n.slider-buttons {\r\n  text-align: center;\r\n  opacity: 0.5;\r\n}\r\n\r\n.btn-next,\r\n.btn-prev {\r\n  opacity: 0.5;\r\n  height: 48px;\r\n  width: 48px;\r\n  border-radius: 50px;\r\n  border: 1px solid;\r\n  /* -webkit-box-shadow: 0 0 12px rgb(0 0 0 / 10%); */\r\n}\r\n\r\n.btn-prev:hover 